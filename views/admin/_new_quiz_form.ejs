
<div class="form-group mb-2">
    <label for="title">Quiz Name</label>
    <input required type="text" name="quizName" id="quizName" class="form-control">
</div>

<div class="form-group mb-2">
    <label for="quizDate">Date</label>
    <input required type="date" name="quizDate" id="quizDate" class="form-control">
</div>

<div class="form-group mb-2">
    <label for="starttime">Start Time</label>
    <input required type="time" id="starttime" name="starttime" class="form-control">
</div>

<div class="form-group mb-2">
    <label for="quizRounds">Rounds</label>
    <input required type="text" name="quizRounds" id="quizRounds" class="form-control">
</div>

<button type="submit" class="btn btn-primary">Save</button>
<a href="/" class="btn btn-secondary">Cancel</a>
